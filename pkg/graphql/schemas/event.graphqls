scalar Upload

type Event {
    id: Int!
    name: String!
    description: String
    image: String
    created_at: Time!
    start_at: Time!
    end_at: Time
}

input NewEvent {
    name: String!
    description: String
    image: Upload
    start_at: Time!
    end_at: Time
}

input UpdateEvent{
    name: String
    description: String
    image: Upload
    start_at: Time
    end_at: Time
}